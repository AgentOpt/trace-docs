{
  "name": "docs-fumadocs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "docs:generate-api": "python3 scripts/generate_api_docs.py",
    "docs:convert-notebooks": "python3 scripts/convert_notebooks.py --all",
    "docs:generate-all": "npm run docs:generate-api && npm run docs:convert-notebooks",
    "prebuild": "npm run docs:generate-all"
  },
  "dependencies": {
    "fumadocs-core": "15.8.3",
    "fumadocs-mdx": "12.0.2",
    "fumadocs-python": "^0.0.5",
    "fumadocs-ui": "15.8.3",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shiki": "^3.13.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  }
}
